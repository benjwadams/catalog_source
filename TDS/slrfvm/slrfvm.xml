      <filter>
        <include wildcard="*.nc"/>
      </filter>
      <addProxies>
        <simpleLatest name="latest.xml" top="true" serviceName="latest"/>
      </addProxies>
    </datasetScan>

    <datasetScan name="Forecast - Individual Files" ID="SLRFVM-Forecast-Files"
      path="SLRFVM-Forecast-Files" location="/var/thredds/SLRFVM/forecast/">
